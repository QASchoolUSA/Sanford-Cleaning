{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp))",
      "dest": "/$1"
    },
    {
      "src": "/residential-cleaning",
      "dest": "/residential-cleaning.html"
    },
    {
      "src": "/commercial-cleaning",
      "dest": "/commercial-cleaning.html"
    },
    {
      "src": "/deep-cleaning",
      "dest": "/deep-cleaning.html"
    },
    {
      "src": "/move-in-move-out-cleaning",
      "dest": "/move-in-move-out-cleaning.html"
    },
    {
      "src": "/post-construction-cleaning",
      "dest": "/post-construction-cleaning.html"
    },
    {
      "src": "/booking",
      "dest": "/booking.html"
    },
    {
      "src": "/get-hired",
      "dest": "/get-hired.html"
    },
    {
      "src": "/free-custom-quote",
      "dest": "/free-custom-quote.html"
    },
    {
      "src": "/faq",
      "dest": "/faq.html"
    },
    {
      "src": "/privacy-policy",
      "dest": "/privacy-policy.html"
    },
    {
      "src": "/terms-of-service",
      "dest": "/terms-of-service.html"
    },
    {
      "src": "/booking-summary",
      "dest": "/index.html"
    },
    {
      "src": "/payment",
      "dest": "/index.html"
    },
    {
      "src": "/booking-success",
      "dest": "/index.html"
    },
    {
      "src": "/get-hired-success",
      "dest": "/index.html"
    }
  ]
}