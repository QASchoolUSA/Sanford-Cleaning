{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev-client\" \"npm:dev-server\"",
    "dev-client": "vite --host",
    "dev-server": "cd server && npm run dev",
    "install-server": "cd server && npm install",
    "build": "npm run install-server && vite build",
    "build:prerender": "npm run install-server && vite build && npm run preview:start && npm run prerender && npm run preview:stop",
    "vercel-build": "npm run install-server && vite build && PRERENDER_BASE_URL=http://localhost:4173 npm run preview:start && sleep 3 && npm run prerender && npm run preview:stop",
    "prerender": "node scripts/prerender.cjs",
    "preview:start": "vite preview --port 4173 &",
    "preview:stop": "pkill -f 'vite preview'",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.20.1",
    "sanitize-html": "^2.17.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^24.0.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-static-copy": "^3.1.3"
  }
}
